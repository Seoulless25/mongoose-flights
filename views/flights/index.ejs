<%- include('../partials/header') %>

 <h2>List of Flights</h2>
 <table>
   <thead>
     <tr>
       <th>Airline</th>
       <th>Airport</th>
       <th>FlightNo</th>
       <th>Departs</th>
     </tr>
   </thead>
   <tbody>
    <%flights.forEach(function(m) { %>
       <tr>
         <td><%= m.airline %></td>
         <td><%= m.airport %></td>
         <td><%= m.flightNo %></td>
         <td><%= m.departs %></td>
       </tr>
     <% }) %>
   </tbody>
 </table>

 <%- include('../partials/header') %>